<h1>
  <%= @tifo.team.name %>
</h1>

<h2>
  By
  <%= @tifo.user.name %>
</h2>

<p>
  <iframe id="sc-widget" width="100%" height="166" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=http%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F<%= @tifo.track_id %>&show_artwork=true"></iframe>
</p>

<ul>
  <% @tifo.photos.each do |photo| %>
    <li>
      <%= image_tag photo.url %>
    </li>
  <% end %>
</ul>

